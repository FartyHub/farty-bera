{
  "name": "@farty-bera/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "tunnel:tg-web": "sudo lt --port 4200 --subdomain farty-bera-tg-web",
    "tunnel:tg-api": "sudo lt --port 3000 --subdomain farty-bera-tg-api",
    "build:api": "npx nx build api --skip-nx-cache",
    "build:tg-api": "npx nx build tg-api --skip-nx-cache",
    "build:web": "npx nx build web --skip-nx-cache",
    "build:tg-web": "npx nx build tg-web --skip-nx-cache",
    "build:tg-web-claim": "npx nx build tg-web-claim --skip-nx-cache",
    "build:functions": "npx nx build functions --skip-nx-cache",
    "build": "npx nx build --skip-nx-cache",
    "web": "npx nx run web:serve --skip-nx-cache",
    "tg-web": "npx nx run tg-web:serve --skip-nx-cache",
    "tg-web-claim": "npx nx run tg-web-claim:serve --skip-nx-cache",
    "api": "npx nx run api:serve --skip-nx-cache",
    "tg-api": "npx nx run tg-api:serve --skip-nx-cache",
    "functions": "npm run build:functions && npx @google-cloud/functions-framework --source=dist/apps/functions --target=updateFartyClawScore",
    "lint:api": "npx nx run api:lint --skip-nx-cache",
    "lint:tg-api": "npx nx run tg-api:lint --skip-nx-cache",
    "lint:web": "npx nx run web:lint --skip-nx-cache",
    "lint:tg-web": "npx nx run tg-web:lint --skip-nx-cache",
    "lint:tg-web-claim": "npx nx run tg-web-claim:lint --skip-nx-cache",
    "lint:all": "npx nx run-many -t lint -p api tg-api web tg-web tg-web-claim --skip-nx-cache",
    "prepare": "husky",
    "create-env": "printenv > ",
    "migration": "npx nx run api:migration-run --skip-nx-cache",
    "generate:api-resource": "npx nx generate @nestjs/schematics:resource --language=ts --path=api/src/app --sourceRoot=apps --no-interactive",
    "deploy:functions": "gcloud config configurations activate farty && npm run build:functions && gcloud functions deploy updateFartyClawScore --region=us-east1 --runtime=nodejs18 --source=dist/apps/functions --trigger-http --allow-unauthenticated",
    "update-sources-windows": "curl http://localhost:3000/api-docs-json > api-docs.json && npx openapi-generator-cli generate -i api-docs.json -g typescript-axios -o libs/api-lib/src",
    "update-sources": "curl http://localhost:3000/api-docs-json > api-docs.json && npx nx run api-lib:generate-sources --skip-nx-cache",
    "update-cors-farty": "gcloud config configurations activate farty && gcloud storage buckets update gs://farty-bera-build --cors-file=cors-config.json",
    "update-cors-tg": "gcloud config configurations activate farty && gcloud storage buckets update gs://tg-mini-app-build --cors-file=cors-config.json",
    "update-cors-tg-eu": "gcloud config configurations activate farty && gcloud storage buckets update gs://tg-mini-app-build-eu --cors-file=cors-config.json",
    "update-cors": "npm run update-cors-farty && npm run update-cors-tg && npm run update-cors-tg-eu"
  },
  "private": true,
  "dependencies": {
    "@argent/x-shared": "^1.32.1",
